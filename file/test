CREATE TABLE `bjcy_order` (
  `order_id` varchar(100) NOT NULL COMMENT '订单号',
  `ext_order_id` varchar(100) DEFAULT NULL COMMENT '外部订单编号',
  `trade_sn` varchar(100) DEFAULT NULL COMMENT '内部交易流水号',
  `ext_trade_sn` varchar(100) DEFAULT NULL COMMENT '外部交易流水号',
  `refund_sn` varchar(100) DEFAULT NULL COMMENT '内部退款流水号',
  `ext_refund_sn` varchar(100) DEFAULT NULL COMMENT '外部退款流水号',
  `activity_name` varchar(100) DEFAULT NULL COMMENT '活动名称',
  `activity_num` int(11) DEFAULT '0' COMMENT '活动编号',
  `order_gen_scene` varchar(200) DEFAULT NULL COMMENT '订单产生场景(汉字描述)',
  `channel_id` varchar(100) DEFAULT NULL COMMENT '流量入口渠道id',
  `f_wx_oid` varchar(50) DEFAULT NULL COMMENT '流量入口渠道wx_openid',
  `f_wx_appid` varchar(50) DEFAULT NULL COMMENT '流量入口渠道wx_appid',
  `pr_cid` varchar(50) DEFAULT NULL COMMENT 'pr_cid',
  `pr_wx_oid` varchar(50) DEFAULT NULL COMMENT 'pr_wx_oid',
  `ppp_cid` varchar(50) DEFAULT NULL COMMENT 'ppp_cid',
  `ppp_wx_oid` varchar(50) DEFAULT NULL COMMENT 'ppp_wx_oid',
  `pp_cid` varchar(50) DEFAULT NULL COMMENT 'pp_cid',
  `pp_wx_oid` varchar(50) DEFAULT NULL COMMENT 'pp_wx_oid',
  `p_cid` varchar(50) DEFAULT NULL COMMENT 'p_cid',
  `p_wx_oid` varchar(50) DEFAULT NULL COMMENT 'p_wx_oid',
  `mobile` varchar(50) DEFAULT NULL COMMENT '手机号',
  `wx_openid` varchar(50) DEFAULT NULL COMMENT 'wx_openid',
  `wx_appid` varchar(50) DEFAULT NULL COMMENT 'wx_appid',
  `jft_trade_token` varchar(50) DEFAULT NULL COMMENT '交易中的trade token',
  `order_status` int(11) DEFAULT '0' COMMENT '订单状态',
  `order_total_amount` varchar(100) DEFAULT NULL COMMENT '订单总金额(元)',
  `item_id` varchar(100) DEFAULT NULL COMMENT 'item_id',
  `sku_id` varchar(100) DEFAULT NULL COMMENT 'sku_id',
  `sku_amount` varchar(100) DEFAULT NULL COMMENT '商品总金额(元)',
  `pck_contract` int(11) DEFAULT '0' COMMENT '合约套餐:1,合约套餐,0,非合约套餐',
  `pck_monthly_fee` int(11) DEFAULT '0' COMMENT '月消',
  `contract_period` int(11) DEFAULT '0' COMMENT '合约期数',
  `pck_name` varchar(100) DEFAULT NULL COMMENT '套餐名称',
  `pck_op_name` varchar(30) DEFAULT NULL COMMENT '运营商使用的套餐名称',
  `pck_details` varchar(300) DEFAULT NULL COMMENT '套餐详细',
  `pck_op_return_fee` int(11) DEFAULT '0' COMMENT '运营商给用户返费',
  `pck_xy_return_fee` int(11) DEFAULT '0' COMMENT '煊煜给用户返费',
  `pck_to_mobile` varchar(100) DEFAULT NULL COMMENT '办理套餐手机号',
  `pck_to_mobile_new` int(11) DEFAULT '0' COMMENT '新用户:1,老用户:0',
  `pck_to_name` varchar(100) DEFAULT NULL COMMENT '办理套餐姓名',
  `pck_to_idcard_num` varchar(100) DEFAULT NULL COMMENT '办理套餐身份在',
  `pck_activation_status` int(11) DEFAULT '0' COMMENT '套餐激活状态:1,激活,0:未激活',
  `pck_effect_time` datetime DEFAULT NULL COMMENT '套餐生效时间',
  `pck_send_fee_status` int(11) DEFAULT '0' COMMENT '返费,冲话费的状态,0:未开始,1,已完成,2,正在返费,3:等待返费',
  `receiver_name` varchar(100) DEFAULT NULL COMMENT '收货人姓名',
  `receiver_mobile` varchar(100) DEFAULT NULL COMMENT '收货人电话',
  `receiver_province` varchar(100) DEFAULT NULL COMMENT '收货人省',
  `receiver_city` varchar(100) DEFAULT NULL COMMENT '收货人市',
  `receiver_area` varchar(100) DEFAULT NULL COMMENT '收货人区',
  `receiver_address` varchar(100) DEFAULT NULL COMMENT '收货人地址',
  `express_code` varchar(50) DEFAULT NULL COMMENT '物流单号',
  `express_name` varchar(50) DEFAULT NULL COMMENT '物流公司',
  `seller_delivery_time` datetime DEFAULT NULL COMMENT '卖家发货时间',
  `buyer_confirm_time` datetime DEFAULT NULL COMMENT '买家签收时间',
  `gift_goods_sku_id` int(11) DEFAULT NULL COMMENT '赠品名称sku_id',
  `gift_goods_name` varchar(200) DEFAULT NULL COMMENT '赠品商品名称',
  `gift_goods_price` varchar(50) DEFAULT NULL COMMENT '赠品价格,元',
  `gift_coupon_id` int(11) DEFAULT NULL COMMENT '赠品名称coupon_id',
  `gift_coupon_name` varchar(200) DEFAULT NULL COMMENT '赠品名称',
  `gift_coupon_type` int(11) DEFAULT NULL COMMENT '礼品类型,1:石油卡,2:天猫券,3:每日优鲜券',
  `gift_coupon_price` varchar(50) DEFAULT NULL COMMENT '赠品价格,元',
  `gift_coupon_return_cycle` int(11) DEFAULT NULL COMMENT '返还周期',
  `gift_coupon_return_cycle_unit` varchar(30) DEFAULT NULL COMMENT '返还周期单位,day,week,month',
  `gift_coupon_return_completed` int(11) DEFAULT '0' COMMENT '返还结束:1,结束,0:未结束',
  `modify_time` datetime DEFAULT NULL COMMENT '修改时间',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  PRIMARY KEY (`order_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='北京城一项目订单列表'